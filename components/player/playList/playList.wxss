.list-container {
    overflow: hidden;
}

.list-top {
    height: 140rpx;
}

.list-title {
    line-height: 70rpx;
    font-size: 38rpx;
    margin-left: 40rpx;
}

.list-num {
    line-height: 70rpx;
    font-size: 26rpx;
    color: gray;
}

.list-nav {
    line-height: 50rpx;
    font-size: 28rpx;
    height: 50rpx;
    margin: 10rpx 0;
}

.list-nav>view {
    margin: 0 20rpx;
}

.list-icon {
    height: 40rpx;
    width: 40rpx;
    margin: 5rpx;
}

.type-info {
    line-height: 50rpx;
    color: white;
    padding: 0 20rpx;
    background: #bebebe;
    border-radius: 25rpx;
    text-align: center;
}

.list-each {
    line-height: 80rpx;
    height: 80rpx;
    margin-bottom: 20rpx;
    font-size: 32rpx;
    position: relative;
}

.list-index {
    width: 60rpx;
    line-height: 80rpx;
    height: 80rpx;
    font-size: 24rpx;
    font-weight: 500;
    text-align: center;
}

.list-info {
    width: 600rpx;
    line-height: 80rpx;
}

.list-name {
    line-height: 80rpx;
    max-width: 400rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.list-singer {
    line-height: 80rpx;
    max-width: 200rpx;
    font-size: 28rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.del {
    position: absolute;
    right: 40rpx;
    height: 80rpx;
    line-height: 80rpx;
    color: gray;
}